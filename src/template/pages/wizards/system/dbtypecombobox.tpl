<div class="custom-select" data-name="dbtype" data-index="0" data-category="database">
    <div class="select-trigger">
        <span class="placeholder">Bitte wählen</span>
        <span class="arrow">▼</span>
    </div>
    <div class="select-dropdown">
        <div class="select-search">
            <input type="search" placeholder="Suchen...">
        </div>
        <div class="select-options">
            <div class="select-option" data-value="">Bitte wählen</div>
            <div class="select-group-header">Relational</div>
            <div class="select-option" data-value="oracle">Oracle Database</div>
            <div class="select-option" data-value="mssql">Microsoft SQL Server</div>
            <div class="select-option" data-value="mysql">MySQL</div>
            <div class="select-option" data-value="mariadb">MariaDB</div>
            <div class="select-option" data-value="postgresql">PostgreSQL</div>
            <div class="select-option" data-value="db2">IBM DB2</div>
            <div class="select-option" data-value="sqlite">SQLite</div>
            <div class="select-group-header">NoSQL</div>
            <div class="select-option" data-value="mongodb">MongoDB</div>
            <div class="select-option" data-value="redis">Redis</div>
            <div class="select-option" data-value="cassandra">Apache Cassandra</div>
            <div class="select-option" data-value="couchdb">CouchDB</div>
            <div class="select-option" data-value="elasticsearch">Elasticsearch</div>
            <div class="select-option" data-value="neo4j">Neo4j</div>
            <div class="select-group-header">In-Memory</div>
            <div class="select-option" data-value="saphana">SAP HANA</div>
            <div class="select-option" data-value="memcached">Memcached</div>
            <div class="select-group-header">Sonstige</div>
            <div class="select-option" data-value="other">Sonstiges</div>
        </div>
    </div>
</div>
